<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="427.0" prefWidth="437.0" spacing="20.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.sub3feb2023.VacationController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ComboBox fx:id="comboBoxLocations" onInputMethodTextChanged="#showOffersOfHotel" prefWidth="150.0" />
   <Button fx:id="buttonGetHotels" mnemonicParsing="false" onAction="#showHotelsInLocation" text="Get Hotels" />

    <Label fx:id="welcomeText" prefHeight="18.0" prefWidth="73.0" />
   <TableView fx:id="tabelHotels" onMouseClicked="#showOffersOfHotel" prefHeight="200.0" prefWidth="200.0" />
</VBox>
